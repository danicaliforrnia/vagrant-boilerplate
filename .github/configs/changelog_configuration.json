  "ignore_labels": [
    "ignore"
  ],
  "sort": {
    "order": "DESC",
    "on_property": "mergedAt"
  },
  "template": "${{CHANGELOG}}\n\nUncategorized (${{UNCATEGORIZED_COUNT}})\n${{UNCATEGORIZED}}\n\nIgnored (${{IGNORED_COUNT}})\n${{IGNORED}}",
  "pr_template": "- **${{TITLE}}** by @${{AUTHOR}} (PR: #${{NUMBER}})",
  "empty_template": "- No changes",
  "max_tags_to_fetch": 200,
  "max_pull_requests": 100,
  "max_back_track_time_days": 730,
  "exclude_merge_branches": [],
  "base_branches": []
}
